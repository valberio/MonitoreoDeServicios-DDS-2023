{{#partial "contenido" }}

<!-- PARTE DE CIERRE DE INCIDENTES -->
<div>
    <div class="row justify-text-center">
        <div class="col-md-12 text-center">
            <h1>Carga de datos </h1>
        </div>
    </div>
</div>

<!-- PARTE DEL SUBIDO DEL ARCHIVO -->
<div class="container mt-5">
    <div class="row">
        <div class="col-md-6 offset-md-3">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">IMPORTE UN ARCHIVO .csv </h5>
                    <form action="/cargadatos" method="POST" enctype="multipart/form-data">
                        <div class="form-group">
                            <input type="file" class="form-control-file" name="archivos" id="archivo">
                        </div>
                        <button type="submit" class="btn btn-primary">Cargar Archivo</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Agrega el enlace a Bootstrap JS (jQuery es requerido) -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
{{/partial}}
{{> templates/baseConNavegador }}